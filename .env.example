# Supabase config
SUPABASE_URL=https://xxxx.supabase.co
SUPABASE_ANON_KEY=your-anon-public-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Viber Bot token for chatbot API
VIBER_BOT_TOKEN=token_here

# Payment Gateway Phone Numbers (Myanmar Kpay / Wave)
KPAY_PHONE_NUMBER=0900000
WAVE_PAY_PHONE_NUMBER=09000000

# Optional: JWT secret for API authentication
JWT_SECRET_KEY=your_jwt_secret_here

# CORS allowed origins (comma separated)
CORS_ORIGINS=http://localhost,http://127.0.0.1,http://your-dashboard-domain.com

# Admin login credentials (for dashboard)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=supersecretpassword
